{
  "name": "unplugin-layui-vue-resolver",
  "version": "0.0.11",
  "description": "layui-vue 按需加载 unplugin 插件",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch src",
    "example:dev": "npm -C examples/vite-layui-vue run dev",
    "build": "tsup",
    "public": "npm publish --access public",
    "prepare": "npm run build"
  },
  "keywords": [
    "layui-vue"
  ],
  "author": "Sight",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.31",
    "typescript": "^4.6.3",
    "tsup": "^5.12.7"
  },
  "dependencies": {
  }
}
